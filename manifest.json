{
  "manifest_version": 2,
  "name": "TabulaRasa",
  "description": "This extension simply opens all links in a new tab",
  "version": "0.1",
  "homepage_url": "https://github.com/tkiefhaber/tabula-rasa",
  "icons": {
    "128": "icon.png"
  },
  "browser_action": {
    "run_at": "document_end",
    "default_icon": "icon.png"
  },
  "content_scripts":[
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["tabula-rasa.js"]
    }
  ]
}
